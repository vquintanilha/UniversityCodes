<Window x:Class="GerenciamentoAgenda.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GerenciamentoAgenda"
        mc:Ignorable="d"
        Title="Gerenciamento de Agenda" Height="480" Width="800">
    <StackPanel>
        <Button x:Name="ContatosButton"
                Margin="15"
                Click="ContatosButton_Click">
            Contatos
        </Button>
        <Button x:Name="CompromissosButton" 
                Margin="15"
                Click="CompromissosButton_Click">
            Compromissos
        </Button>
        <Button x:Name="FinalizarButton" 
                Margin="15"
                Click="FinalizarButton_Click">
            Finalizar Aplicação
        </Button>
    </StackPanel>
</Window>
